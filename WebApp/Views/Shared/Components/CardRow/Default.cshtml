@model Domain.Logic.DTOs.CardDTO;
@using Lang = Binit.Framework.Localization.LocalizationConstants.WebApp.Views.Question.CreateOrEdit
@inject Microsoft.Extensions.Localization.IStringLocalizer<Binit.Framework.SharedResources> localizer



    <div class="card col-md-4">
        <div class="card-body">
            <div class="d-flex p-10 no-block">
                <div class="align-slef-center">
                    <h2 class="m-b-0">@Model.Data</h2>
                    <h6 class="text-muted m-b-0">@Model.Title</h6>
                </div>
                <div class="align-self-center display-6 ml-auto"><i class="text-info icon-Bookmark"></i></div>
            </div>
        </div>
        <div class="progress">
            <div class="progress-bar bg-info" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width:100%; height:3px;"> <span class="sr-only"></span></div>
        </div>
    </div>
